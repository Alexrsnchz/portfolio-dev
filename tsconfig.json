{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@styles/*": [
        "src/assets/styles/*"
      ],
      "@customTypes/*": [
        "src/types/*"
      ],
      "@data/*": [
        "src/assets/data/*"
      ],
      "@icons/*": [
        "src/components/ui/icons/*"
      ],
      "@logos/*": [
        "src/components/ui/logos/*"
      ],
      "@flags/*": [
        "src/components/ui/flags/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  },
  "include": [
    "src",
    "vite.config.ts"
  ]
}